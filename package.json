{
  "name": "jnrmprd",
  "version": "3.0.0",
  "license": "MIT",
  "type": "module",
  "browserslist": [
    "> 0.5%",
    "not IE 11"
  ],
  "scripts": {
    "start": "npx @11ty/eleventy --watch",
    "serve": "npx @11ty/eleventy --serve",
    "build": "ELEVENTY_ENV=production npx @11ty/eleventy",
    "optimize:fonts": "cd src/assets/fonts && glyphhanger --spider-limit=0 --subset=AlbertSans-VariableFont_wght.ttf --formats=woff2"
  },
  "devDependencies": {
    "@11ty/eleventy": "3.0.0-alpha.16",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-rss": "^2.0.1",
    "@11ty/eleventy-plugin-vite": "^5.0.0-alpha.1",
    "autoprefixer": "^10.4.19",
    "clean-css": "^5.3.0",
    "cssnano": "^7.0.3",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-eleventy-img": "^0.9.0",
    "postcss-nesting": "^12.1.5",
    "sass": "^1.52.1"
  },
  "dependencies": {
    "@11ty/eleventy-img": "^2.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "dayjs": "^1.11.5",
    "eleventy-favicon": "jeanremy/eleventy-favicon#patch-1",
    "fast-xml-parser": "^4.3.4",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.5",
    "terser": "^5.15.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.18.0"
  }
}
